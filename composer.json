{
    "name": "dinkbit/lumenpress",
    "description": "The Laravel Lumen Framework with Wordpress.",
    "keywords": ["framework", "laravel", "lumen", "Wordpress"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "johnpbloch/wordpress": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "wordpress-install-dir": "bootstrap/wp"
    },
    "config": {
        "preferred-install": "dist"
    }
}
